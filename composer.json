{
    "name": "project/journal_service_network",
    "description": "journal_service_network",
    "require": {
        "php": ">=8.2",
        "php-di/php-di": "^6.3",
        "psr/http-message": "^1.0",
        "slim/psr7": "*",
        "slim/slim": "^4.7",
        "ext-curl": "*",
        "monolog/monolog": "^3.0",
        "guzzlehttp/guzzle": "^7.5",
        "twig/twig": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Logs\\": "logs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "jetbrains/phpstorm-attributes": "*",
        "symfony/var-dumper": "*",
        "codeception/module-phpbrowser": "*",
        "codeception/module-asserts": "*"
    }
}
